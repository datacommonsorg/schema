rangeIncludes: dcs:FireCauseSpecificEnum

Node: dcid:discoveryDate
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: schema:DateTime
name: "discoveryDate"
description: "The date the fire was confirmed to exist or was reported as discovered."
descriptionUrl: "https://www.nwcg.gov/data-standards-approved"

Node: dcid:controlledDate
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: schema:DateTime
name: "controlledDate"
description: "When the fire was declared controlled, where controlled is defined as: The completion of control line around a fire, any spot fires therefrom, and any interior islands to be saved; burned out any unburned area adjacent to the fire side of control lines; and cool down all hot spots that are immediate threats to the control line, until the lines can be reasonably expected to hold under the foreseeable conditions."
descriptionUrl: "https://www.nwcg.gov/data-standards-approved"

Node: dcid:surfaceArea
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: schema:DateTime
name: "surfaceArea"

Node: dcid:totalDamage
name: "totalDamage"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: schema:Quantity

Node: dcid:expectedLoss
typeOf: dcid:Property
description: "The estimated economic loss resulting from natural hazards. It quantifies loss for relevant consequence types (buildings, people, and agriculture), and in aggregate."
descriptionUrl: "https://hazards.fema.gov/nri/expected-annual-loss"
name: "expectedLoss"
domainIncludes: dcid:NaturalHazardImpact

Node: dcid:containmentDate
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: schema:DateTime
name: "containmentDate"
description: "The date and time a wildfire was declared contained."

Node: dcid:initialResponseDate
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: schema:DateTime
name: "initialResponseDate"
description: "The date/time of the initial response to the incident."

Node: dcid:burnedArea
name: "burnedArea"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: dcs:Quantity
description: "A measure of acres reported for a fire.  More specifically, the number of acres within the current perimeter of a specific, individual incident, including unburned and unburnable islands."

Node: dcid:initialResponseArea
name: "initialResponseArea"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: dcs:Quantity
description: "An estimate of acres burning at the time of initial response. More specifically when the IC arrives and performs initial size up."

Node: dcid:totalIncidentPersonnel
name: "totalIncidentPersonnel"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: dcs:Quantity
description: "The total number of personnel assigned. Includes overhead, crewmembers, helicopter crewmember, engine crewmembers, camp crew people, etc."

Node: dcid:wfigsFireID
name: "wfigsFireID"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
description: "Unique identifier assigned to each wildland fire.  yyyy = calendar year, SSUUUU = POO protecting unit identifier (5 or 6 characters), xxxxxx = local incident identifier (6 to 10 characters) "

Node: dcid:irwinID
name: "irwinID"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
description: "Unique identifier assigned to each incident record in IRWIN(Integrated Reporting of Wildland-Fire Information)."

Node: dcid:incidentLocationDescription
name: "incidentLocationDescription"
typeOf: schema:Property
domainIncludes: dcs:WildlandFireEvent, dcs:PrescribedFireEvent, dcs:FireIncidentComplexEvent
rangeIncludes: dcs:Text
description: "General descriptive location of the incident such as the number of miles from an identifiable town."

